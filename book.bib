  keywords = {Adults,Cooking frequency,Diet quality,fruits and vegetables,Income status,SNAP},
  pages = {1--7},
  file = {ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/58SN6JBU/S0091743515000973.html:}
