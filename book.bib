  year = {2012},
  keywords = {Beverages,Early Childhood Longitudinal Survey,Food stamps,soft drinks,Supplemental Nutrition Assistance Program (SNAP)},
  pages = {1241--1246},
  file = {Fernandes_2012_Effect of the Supplemental Nutrition Assistance Program (SNAP) on Frequency of.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/MF7MPS7A/Fernandes_2012_Effect of the Supplemental Nutrition Assistance Program (SNAP) on Frequency of.pdf:application/pdf;ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/Z3NWWVE7/S2212267212004686.html:}
