  keywords = {advertising,content,cookies,digital goods,information,Internet,online advertising,revenue models},
  file = {Lambrecht et al. - 2014 - How Do Firms Make Money Selling Digital Goods Onli.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/3B6GD47X/Lambrecht et al. - 2014 - How Do Firms Make Money Selling Digital Goods Onli.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/G3KZ7RAH/papers.html:}
