@article{bisitsbullen_positive_2011,
  title = {The positive deviance/hearth approach to reducing child malnutrition: systematic review},
  volume = {16},
  copyright = {{\textcopyright} 2011 Blackwell Publishing Ltd},
  issn = {1365-3156},
  shorttitle = {The positive deviance/hearth approach to reducing child malnutrition},
  doi = {10.1111/j.1365-3156.2011.02839.x},
  abstract = {Objectives\hspace{0.6em} The Positive Deviance/Hearth approach aims to rehabilitate malnourished children using practices from mothers in the community who have well-nourished children despite living in poverty. This study assesses its effectiveness in a range of settings. Methods\hspace{0.6em} Systematic review of peer reviewed intervention trials and grey literature evaluation reports of child malnutrition programs using the Positive Deviance/Hearth approach. Results\hspace{0.6em} Ten peer reviewed studies and 14 grey literature reports met the inclusion criteria. These described results for 17 unique Positive Deviance/Hearth programs in 12 countries. Nine programs used a pre- and post-test design without a control, which limited the conclusions that could be drawn. Eight used more robust designs such as non-randomized trials, non-randomized cross-sectional sibling studies and randomized controlled trials (RCTs). Of the eight programs that reported nutritional outcomes, five reported some type of positive result in terms of nutritional status {\textendash} although the improvement was not always as large as predicted, or across the entire target population. Both the two RCTs demonstrated improvements in carer feeding practices. Qualitative results unanimously reported high levels of satisfaction from participants and recipient communities. Conclusions\hspace{0.6em} Overall this study shows mixed results in terms of program effectiveness, although some Positive Deviance/Hearth programs have clearly been successful in particular settings. Sibling studies suggest that the Positive Deviance/Hearth approach may have a role in preventing malnutrition, not just rehabilitation. Further research is needed using more robust study designs and larger sample sizes. Issues related to community participation and consistency in reporting results need to be addressed.},
  language = {en},
  timestamp = {2015-10-20T18:37:53Z},
  number = {11},
  urldate = {2015-10-20},
  journal = {Tropical Medicine \& International Health},
  author = {{Bisits Bullen}, Piroska A.},
  month = nov,
  year = {2011},
  keywords = {child malnutrition,desnutricin infantil,desviacin positiva,dviance positive,foyer,hearth,malnutrition infantile,Positive deviance,revisin sistemtica,revue systmatique,systematic review,talleres hogareos},
  pages = {1354--1366},
  file = {Bisits Bullen_2011_The positive deviance-hearth approach to reducing child malnutrition.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/AKWWG3CC/Bisits Bullen_2011_The positive deviance-hearth approach to reducing child malnutrition.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/W3SM5IW4/abstract.html:}
}

