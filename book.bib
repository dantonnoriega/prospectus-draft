@article{long_cost_2015,
  title = {Cost {{Effectiveness}} of a {{Sugar-Sweetened Beverage Excise Tax}} in the {{U}}.{{S}}.},
  volume = {49},
  issn = {0749-3797},
  doi = {10.1016/j.amepre.2015.03.004},
  abstract = {Introduction
Reducing sugar-sweetened beverage consumption through taxation is a promising public health response to the obesity epidemic in the U.S. This study quantifies the expected health and economic benefits of a national sugar-sweetened beverage excise tax of {\textdollar}0.01/ounce over 10 years.
Methods
A cohort model was used to simulate the impact of the tax on BMI. Assuming ongoing implementation and effect maintenance, quality-adjusted life-years gained and disability-adjusted life-years and healthcare costs averted were estimated over the 2015{\textendash}2025 period for the 2015 U.S. population. Costs and health gains were discounted at 3\% annually. Data were analyzed in 2014.
Results
Implementing the tax nationally would cost {\textdollar}51 million in the first year. The tax would reduce sugar-sweetened beverage consumption by 20\% and mean BMI by 0.16 (95\% uncertainty interval {[}UI]=0.06, 0.37) units among youth and 0.08 (95\% UI=0.03, 0.20) units among adults in the second year for a cost of {\textdollar}3.16 (95\% UI={\textdollar}1.24, {\textdollar}8.14) per BMI unit reduced. From 2015 to 2025, the policy would avert 101,000 disability-adjusted life-years (95\% UI=34,800, 249,000); gain 871,000 quality-adjusted life-years (95\% UI=342,000, 2,030,000); and result in {\textdollar}23.6 billion (95\% UI={\textdollar}9.33 billion, {\textdollar}54.9 billion) in healthcare cost savings. The tax would generate {\textdollar}12.5 billion in annual revenue (95\% UI={\textdollar}8.92, billion, {\textdollar}14.1 billion).
Conclusions
The proposed tax could substantially reduce BMI and healthcare expenditures and increase healthy life expectancy. Concerns regarding the potentially regressive tax may be addressed by reduced obesity disparities and progressive earmarking of tax revenue for health promotion.},
  timestamp = {2015-11-19T22:25:07Z},
  number = {1},
  urldate = {2015-11-19},
  journal = {American Journal of Preventive Medicine},
  author = {{Long}, Michael W. and {Gortmaker}, Steven L. and {Ward}, Zachary J. and {Resch}, Stephen C. and {Moodie}, Marj L. and {Sacks}, Gary and {Swinburn}, Boyd A. and {Carter}, Rob C. and {Claire Wang}, Y.},
  month = jul,
  year = {2015},
  pages = {112--123},
  file = {ScienceDirect Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/92W9JKT7/S0749379715000963.html:;Long et al_2015_Cost Effectiveness of a Sugar-Sweetened Beverage Excise Tax in the U.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/QT5S8TIC/Long et al_2015_Cost Effectiveness of a Sugar-Sweetened Beverage Excise Tax in the U.pdf:application/pdf}
