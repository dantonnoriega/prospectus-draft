@techreport{grogger_soda_2015,
  type = {Working Paper},
  title = {Soda {{Taxes}} and the {{Prices}} of {{Sodas}} and {{Other Drinks}}: {{Evidence}} from {{Mexico}}},
  shorttitle = {Soda {{Taxes}} and the {{Prices}} of {{Sodas}} and {{Other Drinks}}},
  abstract = {To combat a growing obesity problem, Mexico imposed a nationwide tax on drinks with added sugar, popularly referred to as a {\textquotedblleft}soda tax,{\textquotedblright} effective January 2014. I analyze data on taxed and untaxed products collected as part of Mexico's Consumer Price Index program to estimate how prices responded to the tax. Prices of regular sodas jumped by more than the amount of the tax in the month that the tax took effect. The prices of other taxed drinks also rose, though by a smaller amount. Diet soda prices rose as well, suggesting that consumers may have substituted toward diet sodas after regular sodas became taxable. The prices of bottled water, pure (untaxed) juices, and milk were largely unchanged. A companion analysis of untaxed comparison products showed no general price increases around the time that the soda tax was imposed.},
  timestamp = {2015-11-19T22:45:38Z},
