  author = {{Berry}, Steven T. and {Haile}, Philip A.},
  year = {2014},
  keywords = {_tablet,differentiated products oligopoly,discrete choice models,Nonparametric identification,simultaneous equations models},
  pages = {1749--1797},
  file = {Berry_Haile_2014_Identification in Differentiated Products Markets Using Market Level Data.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/KI6359KP/Berry_Haile_2014_Identification in Differentiated Products Markets Using Market Level Data.pdf:application/pdf;Snapshot:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/MSS82ATW/abstract.html:}
