@article{avena_evidence_2008,
  title = {Evidence for sugar addiction: {{Behavioral}} and neurochemical effects of intermittent, excessive sugar intake},
  volume = {32},
  issn = {0149-7634},
  shorttitle = {Evidence for sugar addiction},
  doi = {10.1016/j.neubiorev.2007.04.019},
  abstract = {{[}Avena, N.M., Rada, P., Hoebel B.G., 2007. Evidence for sugar addiction: Behavioral and neurochemical effects of intermittent, excessive sugar intake. Neuroscience and Biobehavioral Reviews XX(X), XXX{\textendash}XXX]. The experimental question is whether or not sugar can be a substance of abuse and lead to a natural form of addiction. {\textquotedblleft}Food addiction{\textquotedblright} seems plausible because brain pathways that evolved to respond to natural rewards are also activated by addictive drugs. Sugar is noteworthy as a substance that releases opioids and dopamine and thus might be expected to have addictive potential. This review summarizes evidence of sugar dependence in an animal model. Four components of addiction are analyzed. {\textquotedblleft}Bingeing,{\textquotedblright} {\textquotedblleft}withdrawal,{\textquotedblright} {\textquotedblleft}craving{\textquotedblright} and {\textquotedblleft}cross-sensitization{\textquotedblright} are each given operational definitions and demonstrated behaviorally with sugar bingeing as the reinforcer. These behaviors are then related to neurochemical changes in the brain that also occur with addictive drugs. Neural adaptations include changes in dopamine and opioid receptor binding, enkephalin mRNA expression and dopamine and acetylcholine release in the nucleus accumbens. The evidence supports the hypothesis that under certain circumstances rats can become sugar dependent. This may translate to some human conditions as suggested by the literature on eating disorders and obesity.},
  timestamp = {2015-08-10T18:24:52Z},
  number = {1},
  urldate = {2015-08-10},
  journal = {Neuroscience \& Biobehavioral Reviews},
  author = {{Avena}, Nicole M. and {Rada}, Pedro and {Hoebel}, Bartley G.},
  year = {2008},
  keywords = {Acetylcholine,Behavioral sensitization,Binge eating,Craving,Dopamine,Nucleus accumbens,Opioids,Rat,Withdrawal},
  pages = {20--39},
  file = {Avena et al_2008_Evidence for sugar addiction.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/QAHA9WJM/Avena et al_2008_Evidence for sugar addiction.pdf:application/pdf}
}

@article{colditz_economic_1992,
  title = {Economic costs of obesity.},
  volume = {55},
  issn = {0002-9165, 1938-3207},
  abstract = {Approximately 34 million US adults were obese in 1980. Obesity is associated with increased risk of noninsulin-dependent diabetes mellitus (NIDDM), hypertension, cardiovascular disease, gallbladder disease and cholecystectomy, and colon and postmenopausal breast cancer. Using a prevalence-based approach to cost of illness, we estimated the economic costs in 1986 attributable to obesity for these medical conditions. Indirect costs due to morbidity and mortality were discounted at 4\%. Overall, the costs attributable to obesity were {\textdollar}11.3 billion for NIDDM, {\textdollar}22.2 billion for cardiovascular disease, {\textdollar}2.4 billion for gall bladder disease, {\textdollar}1.5 billion for hypertension, and {\textdollar}1.9 billion for breast and colon cancer. Thus a conservative estimate of the economic costs of obesity was {\textdollar}39.3 billion, or 5.5\% of the costs of illness in 1986. Addition of costs due to musculoskeletal disorders could raise this estimate to 7.8\%. The costs of treatment for severe obesity must be weighed against the improved health status and quality of life.},
  language = {en},
  timestamp = {2015-11-24T18:40:25Z},
  number = {2},
