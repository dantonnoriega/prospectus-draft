@article{yangq_added_2014,
  title = {Added sugar intake and cardiovascular diseases mortality among us adults},
  volume = {174},
  issn = {2168-6106},
  doi = {10.1001/jamainternmed.2013.13563},
  abstract = {Importance~
Epidemiologic studies have suggested that higher intake of added sugar is associated with cardiovascular disease (CVD) risk factors. Few prospective studies have examined the association of added sugar intake with CVD mortality.Objective
To examine time trends of added sugar consumption as percentage of daily calories in the United States and investigate the association of this consumption with CVD mortality.Design, Setting, and Participants
National Health and Nutrition Examination Survey (NHANES, 1988-1994 {[}III], 1999-2004, and 2005-2010 {[}n\hspace{0.167em}=\hspace{0.167em}31\hspace{0.167em}147]) for the time trend analysis and NHANES III Linked Mortality cohort (1988-2006 {[}n\hspace{0.167em}=\hspace{0.167em}11\hspace{0.167em}733]), a prospective cohort of a nationally representative sample of US adults for the association study.Main Outcomes and Measures
Cardiovascular disease mortality.Results
Among US adults, the adjusted mean percentage of daily calories from added sugar increased from 15.7\% (95\% CI, 15.0\%-16.4\%) in 1988-1994 to 16.8\% (16.0\%-17.7\%; P\hspace{0.167em}=\hspace{0.167em}.02) in 1999-2004 and decreased to 14.9\% (14.2\%-15.5\%; P\hspace{0.167em}\ensuremath{<}\hspace{0.167em}.001) in 2005-2010. Most adults consumed 10\% or more of calories from added sugar (71.4\%) and approximately 10\% consumed 25\% or more in 2005-2010. During a median follow-up period of 14.6 years, we documented 831 CVD deaths during 163\hspace{0.167em}039 person-years. Age-, sex-, and race/ethnicity{\textendash}adjusted hazard ratios (HRs) of CVD mortality across quintiles of the percentage of daily calories consumed from added sugar were 1.00 (reference), 1.09 (95\% CI, 1.05-1.13), 1.23 (1.12-1.34), 1.49 (1.24-1.78), and 2.43 (1.63-3.62; P\hspace{0.167em}\ensuremath{<}\hspace{0.167em}.001), respectively. After additional adjustment for sociodemographic, behavioral, and clinical characteristics, HRs were 1.00 (reference), 1.07 (1.02-1.12), 1.18 (1.06-1.31), 1.38 (1.11-1.70), and 2.03 (1.26-3.27; P\hspace{0.167em}=\hspace{0.167em}.004), respectively. Adjusted HRs were 1.30 (95\% CI, 1.09-1.55) and 2.75 (1.40-5.42; P\hspace{0.167em}=\hspace{0.167em}.004), respectively, comparing participants who consumed 10.0\% to 24.9\% or 25.0\% or more calories from added sugar with those who consumed less than 10.0\% of calories from added sugar. These findings were largely consistent across age group, sex, race/ethnicity (except among non-Hispanic blacks), educational attainment, physical activity, health eating index, and body mass index.Conclusions and Relevance
Most US adults consume more added sugar than is recommended for a healthy diet. We observed a significant relationship between added sugar consumption and increased risk for CVD mortality.},
  timestamp = {2015-12-04T18:38:09Z},
  number = {4},
  urldate = {2015-12-04},
  journal = {JAMA Internal Medicine},
  author = {{Yang Q} and {Zhang Z} and {Gregg EW} and {Flanders W} and {Merritt R} and {Hu FB}},
  month = apr,
  year = {2014},
  pages = {516--524},
  file = {Yang Q et al_2014_ADded sugar intake and cardiovascular diseases mortality among us adults.pdf:/Users/dnoriega/Library/Application Support/Zotero/Profiles/blfvc19m.default/zotero/storage/3SZIGNVC/Yang Q et al_2014_ADded sugar intake and cardiovascular diseases mortality among us adults.pdf:application/pdf}
}

