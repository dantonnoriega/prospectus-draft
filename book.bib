@techreport{gormley_common_2013,
  address = {Rochester, NY},
  type = {SSRN Scholarly Paper},
  title = {Common {{Errors}}: {{How}} to (and {{Not}} to) {{Control}} for {{Unobserved Heterogeneity}}},
  shorttitle = {Common {{Errors}}},
  abstract = {Controlling for unobserved heterogeneity (or {\textquotedblleft}common errors{\textquotedblright}), such as industry-specific shocks, is a fundamental challenge in empirical research. This paper discusses the limitations of two approaches widely used in corporate finance and asset pricing research: demeaning the dependent variable with respect to the group (e.g., {\textquotedblleft}industry-adjusting{\textquotedblright}) and adding the mean of the group's dependent variable as a control. We show that these methods produce inconsistent estimates and can distort inference. In contrast, the fixed effects estimator is consistent and should be used instead. We also explain how to estimate the fixed effects model when traditional methods are computationally infeasible.Additional programming advice can be found on our websites.},
  timestamp = {2015-03-24T15:41:33Z},
  number = {ID 2023868},
  urldate = {2015-03-24},
